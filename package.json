{
  "name": "csv-explorer",
  "version": "0.1.0",
  "description": "Run SQL queries against CSV documents.",
  "main": "dist/app.js",
  "scripts": {
    "build:ts": "tsc",
    "build:css": "postcss --use tailwindcss --use autoprefixer -o dist/app.css src/**/*.css",
    "build:bundle": "browserify out/index.js -o dist/app.js",
    "build:sql-store": "browserify out/sqlStore.js -o dist/sqlStore.js"
  },
  "author": "Albert Sadowski <asadowski@mailbox.org>",
  "license": "MIT",
  "dependencies": {
    "@dumpstate/web-worker-proxy": "^0.1.5",
    "csv-parse": "^5.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sql.js": "^1.7.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@types/sql.js": "^1.4.3",
    "autoprefixer": "^10.4.7",
    "browserify": "^17.0.0",
    "http-serve": "^1.0.1",
    "postcss": "^8.4.14",
    "postcss-cli": "^10.0.0",
    "tailwindcss": "^3.1.6",
    "typescript": "^4.7.4"
  }
}
